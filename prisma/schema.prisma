generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model EmployeeProfiles {
    id Int @id @default(autoincrement())
    fullName String
    phoneNbr String 
    workPhoneNbr String 
    faxNbr String
    email String 
    jobTitle String 
    company String 
    address String
    qrFilename String?
}